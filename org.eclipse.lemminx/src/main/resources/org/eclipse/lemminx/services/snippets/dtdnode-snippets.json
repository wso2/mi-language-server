{
  "Insert DTD Element Declaration": {
    "prefix": [
      "<!ELEMENT"
    ],
    "suffix": ">",
    "body": [
      "<!ELEMENT ${1:element-name} (${2:#PCDATA})>"
    ],
    "label": "$description",
    "description": "Insert DTD Element Declaration"
  },
  "Insert DTD Attributes List Declaration": {
    "prefix": [
      "<!ATTLIST"
    ],
    "suffix": ">",
    "body": [
      "<!ATTLIST ${1:element-name} ${2:attribute-name} ${3:ID} ${4:#REQUIRED}>"
    ],
    "label": "$description",
    "description": "Insert DTD Attributes List Declaration"
  },
  "Insert Internal DTD Entity Declaration": {
    "prefix": [
      "<!ENTITY"
    ],
    "suffix": ">",
    "body": [
      "<!ENTITY ${1:entity-name} \"${2:entity-value}\">"
    ],
    "label": "$description",
    "description": "Insert Internal DTD Entity Declaration"
  },
  "Insert External DTD Entity Declaration": {
    "prefix": [
      "<!ENTITY"
    ],
    "suffix": ">",
    "body": [
      "<!ENTITY ${1:entity-name} SYSTEM \"${2:entity-value}\">"
    ],
    "label": "$description",
    "description": "Insert External DTD Entity Declaration"
  }
 }